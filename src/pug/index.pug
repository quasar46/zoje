        .feedback 
            ._container
                .feedback__wrap 
                    .feedback__content
                        h2.feedback__title Сделаем ваш выбор проще!
                        p.feedback__text Наши эксперты предоставят профессиональную консультацию по подбору продукции, соответствующей вашим потребностям. Обращайтесь без стеснения – Ваш правильный выбор для нас приоритет!
                    .feedback__form
                        h3.feedback__form-title Заполните данные 
                        form.form(action="") 
                            input.form__input.w50(type="text" placeholder="Имя" required)
                            input.form__input.w50(type="text" placeholder="Фамилия")
                            input.form__input(type="mail" placeholder="@mail")
                            input.form__input(type="tel" placeholder="Номер телефона")
                            input.btn.feedback__submit(type="submit" value="Оставить заявку") 
                    .feedback-btn.btn.header__btn Оставить заявку

